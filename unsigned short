#include <stdio.h>

int main() {
    
   unsigned short int num =65536;
   //int num=2024017464;
   //num=-num;
   printf("Sizeof unsigned short int = %d\n",sizeof (num));
   printf("value = %d", num);
    return 0;
}
OUTPUT
Sizeof unsigned short int = 2
value = 0

